{
  "name": "@institute-alterna/footer-quotes",
  "version": "1.0.1-beta.1",
  "description": "A repository of random quotes to place in website footers or anywhere else as easter eggs.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts,.js",
    "format": "prettier --write '**/*.{ts,js,json,md}'",
    "prepublishOnly": "npm run test && npm run build"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/institute-alterna/footer-quotes.git"
  },
  "homepage": "https://github.com/institute-alterna/footer-quotes#readme",
  "bugs": {
    "url": "https://github.com/institute-alterna/footer-quotes/issues"
  },
  "keywords": [
    "easter-eggs",
    "footer",
    "quotes",
    "internal-tools"
  ],
  "author": "Institute Alterna",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.30",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.12.1",
    "jest": "^30.2.0",
    "prettier": "^3.8.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  },
  "type": "commonjs"
}
